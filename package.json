{
  "name": "@contentstack/delivery-sdk",
  "version": "5.0.0",
  "type": "module",
  "description": "Contentstack Delivery SDK for TypeScript",
  "homepage": "https://github.com/contentstack/contentstack-typescript",
  "author": {
    "name": "Contentstack",
    "url": "https://www.contentstack.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentstack/contentstack-typescript"
  },
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./lib/*": {
      "import": {
        "types": "./dist/types/lib/*.d.ts",
        "default": "./dist/esm/lib/*.js"
      },
      "require": {
        "types": "./dist/types/lib/*.d.ts",
        "default": "./dist/cjs/lib/*.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/cjs/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "jest ./test/unit",
    "test:unit": "jest ./test/unit",
    "test:api": "jest ./test/api",
    "test:all": "jest ./test/unit && jest ./test/api",
    "test:watch": "jest ./test/unit --watch",
    "test:coverage": "jest ./test/unit --coverage",
    "test:sanity-report": "node sanity-report.mjs",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "package": "npm run build && npm pack",
    "husky-check": "npm run build && husky && chmod +x .husky/pre-commit"
  },
  "dependencies": {
    "@contentstack/core": "^1.2.2",
    "axios": "^1.11.0",
    "humps": "^2.0.1"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@nrwl/jest": "^19.8.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@slack/bolt": "^4.4.0",
    "@types/humps": "^2.0.6",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "axios-mock-adapter": "^2.1.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "glob": "^11.0.3",
    "jest": "^30.0.5",
    "jest-html-reporter": "^4.3.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jsdom": "^26.1.0",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
